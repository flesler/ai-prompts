{
  "name": "ai-prompts",
  "version": "1.0.0",
  "type": "module",
  "description": "Chrome extension to organize and quick-insert prompts for AI chat platforms",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:chrome",
    "build:chrome": "tsup --no-dts",
    "build:check": "tsc --noEmit",
    "build:watch": "tsup --watch",
    "build:zip": "cd dist && zip -r ../dist.zip * && cd ..",
    "build:clean": "npx rimraf dist/* *.tsbuildinfo dist.zip",
    "dev": "npm run build:watch",
    "ts": "tsx",
    "eslint": "eslint --cache",
    "lint": "npm run eslint -- src/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "lint:full": "npm run build:check && npm run lint:fix",
    "test": "npm run lint:full && npm run build",
    "prepack": "npm run build:clean && npm run test",
    "publish:dry": "npm pack --dry-run",
    "publish:npm": "npm publish",
    "publish:webstore": "npm run prepack && npm run build:zip && npm run webstore:upload",
    "webstore:auth": "chrome-webstore-upload auth --config .webstore-upload-config.json",
    "webstore:upload": "chrome-webstore-upload upload --source dist.zip --auto-publish false --config .webstore-upload-config.json",
    "webstore:publish": "chrome-webstore-upload publish --config .webstore-upload-config.json",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "keywords": [],
  "author": "Ariel Flesler <aflesler@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flesler/ai-prompts.git"
  },
  "homepage": "https://github.com/flesler/ai-prompts#readme",
  "bugs": {
    "url": "https://github.com/flesler/ai-prompts/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.3",
    "@types/lodash": "^4.14.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "chrome-webstore-upload-cli": "^3.3.2",
    "eslint": "^8.57.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
